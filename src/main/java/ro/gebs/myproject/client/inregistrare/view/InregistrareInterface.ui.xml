<!DOCTYPE ui:UiBinder SYSTEM  "http://dl.google.com/gwt/DTD/XHTML.ENT">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             xmlns:res = 'urn:with:ro.gebs.myproject.client.inregistrare.InregistrareInterfaceResources'>

    <ui:with type = "ro.gebs.myproject.client.inregistrare.view.InregistrareInterfaceResources" field = "res">
    </ui:with>

    <gwt:HTMLPanel>

        <div align = "center">

            <gwt:VerticalPanel res:styleName = "style.background">

                <!--<gwt:Label text = "Bine Ati venit domnilor" res:styleName = "style.blackText" />-->
                <gwt:Label text = "Username" res:styleName = "style.blackText" />
                <gwt:TextBox ui:field="usernameBox" res:styleName = "style.box" />
                <gwt:Label text = "Password" res:styleName = "style.blackText" />
                <gwt:PasswordTextBox ui:field = "passwordBox" res:styleName = "style.box" />
                <gwt:Label text = "Nume" res:styleName = "style.blackText" />
                <gwt:PasswordTextBox ui:field = "numeBox" res:styleName = "style.box" />
                <gwt:Label text = "Prenume" res:styleName = "style.blackText" />
                <gwt:PasswordTextBox ui:field = "prenumeBox" res:styleName = "style.box" />

                <gwt:HorizontalPanel verticalAlignment = "middle">
                    <gwt:Button ui:field = "buttonSubmit" text="Inregistrare"
                                res:styleName = "style.loginButton" />


                    <gwt:CheckBox ui:field = "myCheckBox" />

                    <gwt:Label ui:field = "myLabel" text = "Remember me"
                               res:styleName = "style.blackText" />
                </gwt:HorizontalPanel>

                <gwt:Label ui:field = "completionLabel1" res:styleName = "style.blackText" />
                <gwt:Label ui:field = "completionLabel2" res:styleName = "style.blackText" />

            </gwt:VerticalPanel>


        </div>
    </gwt:HTMLPanel>

</ui:UiBinder>